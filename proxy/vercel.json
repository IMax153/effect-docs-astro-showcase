{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/blog/:match*",
      "destination": "https://effect-docs-astro-showcase.vercel.app/blog/:match*"
    },
    {
      "source": "/blog/",
      "destination": "https://effect-docs-astro-showcase.vercel.app/blog/"
    },
    {
      "source": "/docs/:match*",
      "destination": "https://effect-docs-astro-showcase.vercel.app/docs/:match*"
    },
    {
      "source": "/docs/",
      "destination": "https://effect-docs-astro-showcase.vercel.app/docs/getting-started/introduction"
    },
    {
      "source": "/_astro/:match*",
      "destination": "https://effect-docs-astro-showcase.vercel.app/_astro/:match*"
    },
    {
      "source": "/",
      "destination": "https://website-eight-rho-75.vercel.app/"
    }
  ]
}
